<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddWordController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/AddWordBackground.png" />
         </image>
      </ImageView>
      <Label layoutX="218.0" layoutY="28.0" prefHeight="31.0" prefWidth="165.0" text="Add New Word" textAlignment="CENTER">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Button layoutX="218.0" layoutY="321.0" mnemonicParsing="false" onAction="#addNewWord" text="Add" />
      <Button layoutX="331.0" layoutY="321.0" mnemonicParsing="false" onAction="#changeSceneSample" text="Cancel" />
      <TextField fx:id="newEnglishId" layoutX="166.0" layoutY="109.0" prefHeight="25.0" prefWidth="311.0" />
      <TextField fx:id="newVietnameseId" layoutX="166.0" layoutY="200.0" prefHeight="72.0" prefWidth="311.0" />
      <Label layoutX="52.0" layoutY="109.0" prefHeight="25.0" prefWidth="73.0" text="EngLish" textFill="#e81010">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="52.0" layoutY="200.0" prefHeight="25.0" prefWidth="82.0" text="Vietnamese" textFill="#2414d7">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
